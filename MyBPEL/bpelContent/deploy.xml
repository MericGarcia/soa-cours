<?xml version="1.0" encoding="UTF-8"?>
<deploy xmlns="http://www.apache.org/ode/schemas/dd/2007/03" xmlns:fr.keyconslting.ws="http://fr.keyconslting.ws" xmlns:service1.formation.keyconsulting.fr="http://service1.formation.keyconsulting.fr/" xmlns:service2.formation.keyconsulting.fr="http://service2.formation.keyconsulting.fr/">
  <process name="fr.keyconslting.ws:WS_invocations">
    <active>true</active>
    <retired>false</retired>
    <process-events generate="all"/>
    <provide partnerLink="client">
      <service name="fr.keyconslting.ws:WS_invocationsService" port="WS_invocationsPort"/>
    </provide>
    <invoke  partnerLink="WS1">
      <service name="service1.formation.keyconsulting.fr:HelloServiceService" port="HelloServicePort"/>
    </invoke >
    <invoke  partnerLink="WS2">
      <service name="service2.formation.keyconsulting.fr:HelloService2Service" port="HelloService2Port"/>
    </invoke >
    <invoke partnerLink="WS1">
      <service name="service1.formation.keyconsulting.fr:HelloServiceService" port="HelloServicePort"/>
    </invoke>
    <invoke partnerLink="WS2">
      <service name="service2.formation.keyconsulting.fr:HelloService2Service" port="HelloService2Port"/>
    </invoke>
  </process>
</deploy>